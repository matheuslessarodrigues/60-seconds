function fade(fa)
	fa=max(min(1,fa),0)

	local fn=8
	local pn=15
	local fc=1/fn
	local fi=flr(fa/fc)+1

	for n=1,pn do
		pal(n,fades[n][fi],0)
	end
end

fade_time=10
